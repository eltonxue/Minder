<!DOCTYPE html>
<html>
  <head>
    <title>Minder</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <link rel="stylesheet" type="text/css" href="stylesheets/signup-login-page.css">
    <link rel="stylesheet" type="text/css" href="stylesheets/reusable.css">
    <link href="https://fonts.googleapis.com/css?family=Pacifico|Didact+Gothic" rel="stylesheet">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
  </head>

  <body>
    <div class="center-content">
      <h2>Minder</h2>
    </div>
    <div class="center-content">
      <form id="login-form">
        <div>
          <label>Email</label>
          <input id="login-email"></input>
        </div>
        <div>
          <label>Password</label>
          <input id="login-password" type="password"></input>
        </div>
        <div class="center-content">
          <input type="submit" value="Login" id="login" class="button-submit"></input>
        </div>
      </form>
    </div>
    <div class="center-content">
      <form id="signup-form">
        <div>
          <label>Name</label>
          <input id="signup-name"></input>
        </div>
        <div>
          <label>Email</label>
          <input id="signup-email"></input>
        </div>
        <div>
          <label>Password</label>
          <input id="signup-password" type="password"></input>
        </div>
        <div>
          <label>Confirm Password</label>
          <input id="signup-confirm-password" type="password"></input>
        </div>
        <div class="center-content">
          <input type="submit" value="Sign Up" id="signup" class="button-submit"></input>
        </div>
      </form>
    </div>
  </body>
  <footer>
    <script src="javascripts/signup-login-page.js"></script>
  </footer>
</html>
